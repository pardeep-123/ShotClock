<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_home"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.app.shotclock.fragments.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_subscriptionFragment"
            app:destination="@id/subscriptionFragment" />
        <action
            android:id="@+id/action_homeFragment_to_myRequestsFragment"
            app:destination="@id/myRequestsFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.app.shotclock.fragments.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile">
        <action
            android:id="@+id/action_profileFragment_to_editProfileFragment"
            app:destination="@id/editProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.app.shotclock.fragments.EditProfileFragment"
        android:label="fragment_edit_profile"
        tools:layout="@layout/fragment_edit_profile" />
    <fragment
        android:id="@+id/myRequestsFragment"
        android:name="com.app.shotclock.fragments.MyRequestsFragment"
        android:label="fragment_my_requests"
        tools:layout="@layout/fragment_my_requests" >
        <action
            android:id="@+id/action_myRequestsFragment_to_speedDateSessionFragment"
            app:destination="@id/speedDateSessionFragment" />
        <action
            android:id="@+id/action_myRequestsFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_myRequestsFragment_to_videoCallFragment"
            app:destination="@id/videoCallFragment" />
    </fragment>
    <fragment
        android:id="@+id/notificationFragment"
        android:name="com.app.shotclock.fragments.NotificationFragment"
        android:label="fragment_notification"
        tools:layout="@layout/fragment_notification" >
        <action
            android:id="@+id/action_notificationFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/messageFragment"
        android:name="com.app.shotclock.fragments.MessageFragment"
        android:label="fragment_message"
        tools:layout="@layout/fragment_message" >
        <action
            android:id="@+id/action_messageFragment_to_chatFragment"
            app:destination="@id/chatFragment" />
    </fragment>
    <fragment
        android:id="@+id/chatFragment"
        android:name="com.app.shotclock.fragments.ChatFragment"
        android:label="fragment_chat"
        tools:layout="@layout/fragment_chat">
        <action
            android:id="@+id/action_chatFragment_to_videoCallFragment"
            app:destination="@id/videoCallFragment" />
    </fragment>
    <fragment
        android:id="@+id/changePasswordFragment"
        android:name="com.app.shotclock.fragments.ChangePasswordFragment"
        android:label="fragment_change_password"
        tools:layout="@layout/fragment_change_password" />
    <fragment
        android:id="@+id/cookiePolicyFragment"
        android:name="com.app.shotclock.fragments.CookiePolicyFragment"
        android:label="fragment_cookie_policy"
        tools:layout="@layout/fragment_cookie_policy" />
    <fragment
        android:id="@+id/copyrightPolicyFragment"
        android:name="com.app.shotclock.fragments.CopyrightPolicyFragment"
        android:label="fragment_copyright_policy"
        tools:layout="@layout/fragment_copyright_policy" />
    <fragment
        android:id="@+id/privacyPolicyFragment"
        android:name="com.app.shotclock.fragments.PrivacyPolicyFragment"
        android:label="fragment_privcay_policy"
        tools:layout="@layout/fragment_privcay_policy" />
    <fragment
        android:id="@+id/safeDatingPolicyFragment"
        android:name="com.app.shotclock.fragments.SafeDatingPolicyFragment"
        android:label="fragment_safe_dating_policy"
        tools:layout="@layout/fragment_safe_dating_policy" />
    <fragment
        android:id="@+id/termsConditionsFragment"
        android:name="com.app.shotclock.fragments.TermsConditionsFragment"
        android:label="fragment_terms_conditions"
        tools:layout="@layout/fragment_terms_conditions" />
    <fragment
        android:id="@+id/subscriptionFragment"
        android:name="com.app.shotclock.fragments.SubscriptionFragment"
        android:label="fragment_subscription"
        tools:layout="@layout/fragment_subscription" />
    <fragment
        android:id="@+id/videoCallFragment"
        android:name="com.app.shotclock.fragments.VideoCallFragment"
        android:label="fragment_video_call"
        tools:layout="@layout/fragment_video_call" >
        <action
            android:id="@+id/action_videoCallFragment_to_icebreakerQuestionsFragment"
            app:destination="@id/icebreakerQuestionsFragment" />
    </fragment>
    <fragment
        android:id="@+id/icebreakerQuestionsFragment"
        android:name="com.app.shotclock.fragments.IcebreakerQuestionsFragment"
        android:label="fragment_icebraker_questions"
        tools:layout="@layout/fragment_icebraker_questions" />
    <fragment
        android:id="@+id/speedDateSessionFragment"
        android:name="com.app.shotclock.fragments.SpeedDateSessionFragment"
        android:label="fragment_speed_date_session"
        tools:layout="@layout/fragment_speed_date_session" >
        <action
            android:id="@+id/action_speedDateSessionFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_speedDateSessionFragment_to_videoCallFragment"
            app:destination="@id/videoCallFragment" />
    </fragment>
    <fragment
        android:id="@+id/incomingCallActivity"
        android:name="com.app.shotclock.videocallingactivity.IncomingCallActivity"
        android:label="IncomingCallActivity" >
        <action
            android:id="@+id/action_incomingCallActivity_to_videoCallFragment"
            app:destination="@id/videoCallFragment" />
    </fragment>


</navigation>